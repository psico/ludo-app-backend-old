type Query {
    bookById(id: ID): Book
    userInfoId(id: String): UserInfo
    usersInfo: [UserInfo]
    matches: [Match]
}

type Book {
    id: ID
    name: String
    pageCount: Int
    author: Author
}

type Author {
    id: ID
    firstName: String
    lastName: String
}

type Friend {
    uid: ID
    name: String
}

type UserInfo {
    uid: ID
    name: String
    friends:[Friend!]!
}

type Match {
    uid: ID
    gameMoment: String
    players: [Friend!]
}
