type Query {
    bookById(id: ID): Book
}

type Book {
    id: ID
    name: String
    pageCount: Int
    author: Author
}

type Author {
    id: ID
    firstName: String
    lastName: String
}

type Friend {
    uid: ID
    name: String
}

type UserInfo {
    uid: ID
    name: String
    friends:[Friend]
}
